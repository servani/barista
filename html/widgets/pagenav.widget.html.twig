{% if pagenav.results %}
	{% if pagenav.nofpages > 1 %}
		<div class="pagination-centered">
			<ul class="pagination">
				<li class="arrow {{ pagenav.page == 1 ? 'unavailable' : '' }}">
					{% if pagenav.page == 1 %}
						<a href="#">&laquo;</a>
					{% else %}
						<a href="{{ url('admin/list/' ~ en) }}?p={{ pagenav.page - 1}}&amp;o={{ pagenav.order }}&amp;d={{ pagenav.dir }}&amp;w={{ pagenav.where }}&amp;cw={{ pagenav.customWhere }}">&laquo;</a>
					{% endif %}
				</li>
				{% set aux = true %}
				{% set aux2 = false %}
				{% set min = 4 %}
				{% set max = pagenav.nofpages - 3 %}
				{% set midmin = pagenav.page - 2 %}
				{% set midmax = pagenav.page + 2 %}
				{% if pagenav.nofpages < 10 %}
					{% set min = 0 %}
					{% set aux = false %}
				{% endif %}
				{% for i in 1..pagenav.nofpages %}
					{% if not min or i < min or i > max or (i > midmin and i < midmax) %}
						<li class="{{ pagenav.page == i ? 'current' : '' }}"><a href="{{ url('admin/list/' ~ en) }}?p={{ i }}&amp;o={{ pagenav.order }}&amp;d={{ pagenav.dir }}&amp;w={{ pagenav.where }}&amp;cw={{ pagenav.customWhere }}">{{ i }}</a></li>
						{% if i > min and i < max and (i > midmin and i < midmax) %}
							{% set aux2 = true %}
						{% endif %}
					{% elseif aux or aux2 %}
						<li>...</li>
						{% set aux = false %}
						{% set aux2 = false %}
					{% endif %}
				{% endfor %}
				<li class="arrow {{ pagenav.page == pagenav.nofpages ? 'unavailable' : '' }}">
					{% if pagenav.page == pagenav.nofpages %}
						<a href="#">&raquo;</a>
					{% else %}
						<a href="{{ url('admin/list/' ~ en) }}?p={{ pagenav.page + 1}}&amp;o={{ pagenav.order }}&amp;d={{ pagenav.dir }}&amp;w={{ pagenav.where }}&amp;cw={{ pagenav.customWhere }}">&raquo;</a>
					{% endif %}
				</li>
			</ul>
		</div>
	{% endif %}
	<p>Cantidad de resultados por pÃ¡gina:</p>
	<form id="results-page" class="form-horizontal">
		<div class="form-group">
			<div class="col-sm-2">
				<input type="text" class="form-control" placeholder="{{ pagenav.max }}">
				<a href="{{ url('admin/list/' ~ en) }}?o={{ pagenav.order }}&amp;d={{ pagenav.dir }}&amp;w={{ pagenav.where }}&amp;cw={{ pagenav.customWhere }}&amp;n=" class="btn btn-default">Listar</a>
			</div>
		</div>
	</form>
{% endif %}
{% if not pagenav.results %}
	<p class="no-results">No se encontraron resultados</p>
{% endif %}